#!/bin/bash
#SBATCH -p radclouds
#SBATCH -N 1
#SBATCH -n 56
#SBATCH --nodelist=c874
#SBATCH --exclusive
#SBATCH --output=outbatch.out%j.txt
#SBATCH --error=outbatch.err%j.txt
#SBATCH -t 60:00:00 # max job run time HH:MM:SS

# V2
# When you run this slurm job, make your arguement the path to the raw directory that you want to run ncrcat
ncrcat $1/wrfout_d01* $1/d01
ncrcat $1/wrfout_d02* $1/d02

